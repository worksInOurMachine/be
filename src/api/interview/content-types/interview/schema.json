{
  "kind": "collectionType",
  "collectionName": "interviews",
  "info": {
    "singularName": "interview",
    "pluralName": "interviews",
    "displayName": "interview"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "resume": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mode": {
      "type": "enumeration",
      "default": "Technical",
      "enum": [
        "HR",
        "Technical"
      ]
    },
    "difficulty": {
      "type": "enumeration",
      "default": "medium",
      "enum": [
        "easy",
        "hard",
        "medium"
      ]
    },
    "skills": {
      "type": "string"
    },
    "details": {
      "type": "text"
    },
    "conversation": {
      "type": "json"
    },
    "report": {
      "type": "json"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "interviews"
    },
    "numberOfQuestions": {
      "type": "integer"
    },
    "report": {
      "type": "richtext"
    }
  }
}
